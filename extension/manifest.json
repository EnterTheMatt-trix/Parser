{
    "name": "Skill Parser Extension",
    "description": "A simple Chrome extension that parses pages for skills. On demand.",
    "version": "3.1",
    "manifest_version": 3,
    "permissions": ["activeTab", "scripting", "storage"],
    "action": {
      "default_title": "Find Skills",
      "default_popup": "popup.html"
    },
    "host_permissions": ["<all_urls>"],
    "web_accessible_resources": [
    {
      "resources": ["flat_technologies.json"],
      "matches": ["<all_urls>"]
    }
  ]
}